@page "/logout"
@inject AuthenticationStateProvider AuthenticationProvider
<PageTitle>@Loc["logout"]</PageTitle>

@code {

  [CascadingParameter]
  public Task<AuthenticationState> AuthTask { get; set; }

  protected override async Task OnInitializedAsync() {
    await ((AdminAuthenticationStateProvider)AuthenticationProvider).Logout();
    Nav.NavigateTo(Nav.Uri.Replace("logout", ""), true);
  }

}